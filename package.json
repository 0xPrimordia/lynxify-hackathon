  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-scripts": "tsc --project tsconfig.scripts.json",
    "init-hcs": "npm run build-scripts && node dist/app/scripts/init-hcs.js",
    "agent": "npm run build-scripts && node dist/app/scripts/run-agent.js",
    "rebalance-agent": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' src/app/scripts/rebalance-agent.ts",
    "build-server": "tsc --project tsconfig.server.json",
    "ws": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' server.ts",
    "demo": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' combined-server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
