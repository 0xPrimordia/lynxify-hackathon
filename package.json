{
  "name": "lynxify-hackathon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "node combined-server.js",
    "start:agent": "node scripts/restart-agent.mjs",
    "test:api-endpoints": "node scripts/test-api-endpoints.mjs",
    "test:message": "node scripts/test-agent-message.mjs",
    "test:direct-message": "node scripts/direct-message-test.mjs",
    "test:connection": "node scripts/create-connection.mjs",
    "lint": "next lint",
    "build-scripts": "tsc --project tsconfig.scripts.json",
    "init-hcs": "npm run build-scripts && node dist/app/scripts/init-hcs.js",
    "agent": "npm run build-scripts && node dist/app/scripts/run-agent.js",
    "rebalance-agent": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' src/app/scripts/rebalance-agent.ts",
    "build-server": "tsc --project tsconfig.server.json",
    "ws": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' server.ts",
    "demo": "node combined-server.js",
    "demo:refactored": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' src/app/scripts/start-server.ts",
    "unified-server": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' src/app/scripts/unified-server.ts",
    "test": "jest --testTimeout=60000 --detectOpenHandles",
    "test:services": "jest src/__tests__/services/lynxify-agent.test.ts --forceExit",
    "test:all-services": "jest src/__tests__/services/ --testPathIgnorePatterns=\"/src/__tests__/services/agents/\" --forceExit",
    "test:agents": "jest src/__tests__/services/agents/ --forceExit",
    "test:types": "jest src/__tests__/types/ --forceExit",
    "test:utils": "jest src/__tests__/services/event-emitter.test.ts --forceExit",
    "test:core": "npm run test:services && npm run test:api && npm run test:types && npm run test:utils",
    "test:react": "jest --config=jest.react.config.js --testTimeout=60000 --detectOpenHandles",
    "test:e2e-simple": "NODE_ENV=test npx jest src/__tests__/e2e/mock-e2e.test.ts src/__tests__/integration/hcs10-messaging.test.ts --testPathIgnorePatterns=\"[]\" --forceExit",
    "test:integration-simple": "NODE_ENV=test npx jest src/__tests__/integration/agent-registration.test.ts src/__tests__/integration/hcs10-agent-communication.test.ts --testPathIgnorePatterns=\"[]\" --forceExit",
    "test:watch": "jest --watch --testTimeout=60000 --detectOpenHandles",
    "test:ci": "jest --ci --testTimeout=60000 --detectOpenHandles",
    "test:hcs10": "jest src/__tests__/services/hcs10-protocol.test.ts",
    "server:hcs10": "./scripts/start-server.sh",
    "build-and-test-hcs10": "npm run build-server && npm run test:hcs10",
    "test:refactored": "jest src/__tests__/refactored/",
    "test:e2e": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' src/app/scripts/test-e2e.ts",
    "build:hcs10": "tsc --project tsconfig.server.json",
    "build:hcs10-agent": "tsc --project tsconfig.hcs10.json",
    "start:hcs10-agent": "npm run build:hcs10-agent && node scripts/run-hcs10-agent.js",
    "test:hcs10-agent": "npm run build:hcs10-agent && node scripts/test-hcs10-agent.js",
    "check:hcs10-agent": "node scripts/simple-hcs10-test.js",
    "full-hcs10-agent-test": "npm run build:hcs10-agent && npm run start:hcs10-agent & sleep 10 && npm run test:hcs10-agent && npm run check:hcs10-agent",
    "hcs10:register": "node scripts/hcs10/register-agent.mjs",
    "hcs10:verify": "node scripts/hcs10/verify-agent.mjs",
    "hcs10:monitor": "node scripts/hcs10/monitor-agent.mjs",
    "hcs10:full-flow": "npm run hcs10:register && npm run hcs10:verify && npm run hcs10:monitor",
    "hcs10:start-server": "node scripts/start-hcs10-server.mjs",
    "hcs10:update-env": "node scripts/hcs10/update-env.mjs",
    "hcs10:server": "node combined-server.js",
    "generate-docs": "typedoc --out docs src/app",
    "prepare-demo": "ts-node scripts/prepare-demo.ts"
  },
  "dependencies": {
    "@hashgraph/sdk": "^2.63.0",
    "@hashgraphonline/standards-sdk": "^0.0.95",
    "@nextui-org/react": "^2.6.11",
    "@tanstack/react-query": "^5.74.4",
    "chart.js": "^4.4.9",
    "next": "15.3.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.96.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-module-resolver": "^5.0.2",
    "cypress": "^14.3.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
